{
    "manifest_version": 2,
    "name": "AnimeFLV Bleach Extension",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "https://www3.animeflv.net/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://www3.animeflv.net/ver/bleach-tv-*"
        ],
        "js": [
          "content.js"
        ]
      }
    ]
  }
  